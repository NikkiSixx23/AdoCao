<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Adocao"
        tools:targetApi="31">
        <activity
            android:name=".ActivityClass.recycleCaoActivity"
            android:exported="false" />
        <activity
            android:name=".ActivityClass.AdotadoActivity"
            android:exported="false" />
        <activity
            android:name=".ActivityClass.CadastrarUsuarioActivity"
            android:exported="false" />
        <activity
            android:name=".ActivityClass.HistoricoActivity"
            android:exported="false" />
        <activity
            android:name=".ActivityClass.CaoActivity"
            android:exported="false" />
        <activity
            android:name=".ActivityClass.CadastrarCaoActivity"
            android:exported="false" />
        <activity
            android:name=".ActivityClass.PerfilActivity"
            android:exported="false" />
        <activity
            android:name=".ActivityClass.AdocaoActivity"
            android:exported="false" />
        <activity
            android:name=".ActivityClass.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".ActivityClass.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>